    </sql>

    <select id="selectContractContentInfoList" parameterType="ContractContentInfo" resultMap="ContractContentInfoResult">
        <include refid="selectContractContentInfoVo"/>
        <where>  
            <if test="contractName != null  and contractName != ''"> and contract_name like concat('%', #{contractName}, '%')</if>
            <if test="contractId != null  and contractId != ''"> and contract_id = #{contractId}</if>
            <if test="signDate != null "> and sign_date = #{signDate}</if>
            <if test="params.beginContractTotal != null and params.beginContractTotal != '' and params.endContractTotal != null and params.endContractTotal != ''"> and contract_total between #{params.beginContractTotal} and #{params.endContractTotal}</if>
        </where>
    </select>
    
    <select id="selectContractContentInfoByGoodsId" parameterType="String" resultMap="ContractContentInfoResult">
